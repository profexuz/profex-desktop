<UserControl x:Class="Profex_Desktop.Components.MastersInfo.MastersSkills"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Profex_Desktop.Components.MastersInfo"
             mc:Ignorable="d" 
             Height="30" Width="auto" Background="Transparent" Margin="3">
    <Border Background="{DynamicResource PrimaryBackground}"
            CornerRadius="15">
    <Grid>
        <Border CornerRadius="10" HorizontalAlignment="Left" Margin="10 0"
                Background="White" Height="10" Width="10"/>
        <Label x:Name="lblSkill" Foreground="White" FontSize="12"
               HorizontalAlignment="Left" Margin="25 0 10 0"/>
        </Grid>
    </Border>
</UserControl>
