    <!--Label Sure Name-->
    <Style x:Key="OnlineMaster" TargetType="{x:Type Label}">
        <Setter Property="Foreground" Value="{DynamicResource lblMastersOnlineColor}"/>
        <Setter Property="FontFamily" Value="{DynamicResource FontFamilyNormal}"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="FontSize" Value="14"/>
        <Setter Property="Height" Value="20"/>
        <Setter Property="Height" Value="50"/>
        <Setter Property="VerticalAlignment" Value="Bottom"/>
        <Setter Property="HorizontalAlignment" Value="Right"/>
        <Setter Property="Content" Value="{DynamicResource labelMasterOnlineHeader}"/>
        <Setter Property="Margin" Value="0 0 0 5"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Label}">
                    <Border x:Name="B" BorderThickness="0">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="20"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <Path Grid.Column="0"
                                  Data="{DynamicResource server}"
                                  Fill="{DynamicResource lblMastersOnlineColor}"
                                  Opacity="0.5" Height="12" Width="12"
                                  Stretch="Uniform" HorizontalAlignment="Right" Margin="3"/>
                            <Label Grid.Column="1" Content="{TemplateBinding Content}" FontSize="{TemplateBinding FontSize}"
                                   Foreground="{TemplateBinding Foreground}" FontFamily="{TemplateBinding FontFamily}"
                                   Padding="{TemplateBinding Padding}" VerticalAlignment="Center"/>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>


